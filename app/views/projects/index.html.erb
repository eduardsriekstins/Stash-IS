<div class="container">
  <br>
  <h1>Projects</h1>
  <br>
  <div class="container">
    <%= link_to "New project", new_project_path, class: "btn btn-dark" %>
  </div>
  <br>

  <div id="projects" class="container">
    <% @projects.each do |project| %>
      <%= render project %>
      <p>
        <%= link_to "Show this project", project, class: "btn btn-dark" %>
      </p>
    <% end %>
  </div>

</div>